# CampusConnect Environment Variables
# Copy this file to .env and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=your-supabase-url-here
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# Firebase Configuration
VITE_FIREBASE_PROJECT_ID=your-firebase-project-id-here
VITE_FIREBASE_API_KEY=your-firebase-api-key-here
VITE_FIREBASE_AUTH_DOMAIN=your-firebase-auth-domain-here
VITE_FIREBASE_MESSAGING_SENDER_ID=your-firebase-messaging-sender-id-here

# Sentry Configuration
VITE_SENTRY_DSN=your-sentry-dsn-here

# App Configuration
VITE_APP_VERSION=1.0.0
VITE_APP_NAME=CampusConnect
